{
  "name": "one_batch_test",
  "n_gpu": 1,
  "preprocessing": {
    "sr": 22050
  },
  "arch": {
    "generator": {
      "type": "HiFiGenerator",
      "args": {
        "ku": [16, 16, 4, 4],
        "kr": [3, 7, 11],
        "Dr":[[[1, 1], [3, 1], [5, 1]],
              [[1, 1], [3, 1], [5, 1]],
              [[1, 1], [3, 1], [5, 1]]]
      }
    },
    "discriminator": {
      "type": "HiFiDiscriminator",
      "args": {}
    }
  },
  "data": {
    "train": {
      "batch_size": 3,
      "num_workers": 0,
      "datasets": [
        {
          "type": "LJspeechDataset",
          "args": {
            "part": "train",
            "limit": 3
          }
        }
      ]
    },
    "val": {
      "batch_size": 3,
      "num_workers": 0,
      "datasets": [
        {
          "type": "LJspeechDataset",
          "args": {
            "part": "train",
            "limit": 3
          }
        }
      ]
    }
  },
  "optimizers": {
    "optimizer_G": {
      "type": "SGD",
      "args": {
        "lr": 1e-2
      }
    },
    "optimizer_D": {
      "type": "SGD",
      "args": {
        "lr": 1e-2
      }
    }
  },
  "loss": {
    "loss_D": {
      "type": "DiscriminatorLoss",
      "args": {}
    },
    "loss_G": {
      "type": "GeneratorLoss",
      "args": {}
    }
  },
  "metrics": [],
  "lr_scheduler": {
    "lr_scheduler_G": {
      "type": "OneCycleLR",
      "args": {
        "steps_per_epoch": 10,
        "epochs": 10,
        "anneal_strategy": "cos",
        "max_lr": 1e-2,
        "pct_start": 0.2
      }
    },
    "lr_scheduler_D": {
      "type": "OneCycleLR",
      "args": {
        "steps_per_epoch": 10,
        "epochs": 10,
        "anneal_strategy": "cos",
        "max_lr": 1e-2,
        "pct_start": 0.2
      }
    }
  },
  "trainer": {
    "epochs": 10,
    "save_dir": "saved/",
    "save_period": 100,
    "verbosity": 2,
    "monitor": "min val_loss",
    "visualize": "wandb",
    "wandb_project": "HiFiGan_project",
    "len_epoch": 10
  }
}
